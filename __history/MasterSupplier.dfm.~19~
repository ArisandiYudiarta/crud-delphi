  object TableQuerySupplier: TFDQuery
    Active = True
    CachedUpdates = True
    Connection = DM.FDConnection
    UpdateObject = FDUpdateSQLSupplier
    SQL.Strings = (
      'select * from'
      'Supplier')
    Left = 544
    Top = 200
  end
  object FDUpdateSQLSupplier: TFDUpdateSQL
    Connection = DM.FDConnection
    InsertSQL.Strings = (
      'INSERT INTO TRIAL.dbo.Supplier'
      '(Kode_Sup, Nama_Sup, Alamat, Telp, Fax, '
      '  Kota, Contact_Person, Jangka_Waktu, Buffer_Stock, '
      '  Keterangan, No_Rekening, Nama_Bank, Jenis_PKP, '
      '  NPWP, NPPKP, Nama_Perusahaan, Alamat_Perusahaan)'
      
        'VALUES (:NEW_Kode_Sup, :NEW_Nama_Sup, :NEW_Alamat, :NEW_Telp, :N' +
        'EW_Fax, '
      
        '  :NEW_Kota, :NEW_Contact_Person, :NEW_Jangka_Waktu, :NEW_Buffer' +
        '_Stock, '
      
        '  :NEW_Keterangan, :NEW_No_Rekening, :NEW_Nama_Bank, :NEW_Jenis_' +
        'PKP, '
      
        '  :NEW_NPWP, :NEW_NPPKP, :NEW_Nama_Perusahaan, :NEW_Alamat_Perus' +
        'ahaan);'
      'SELECT Jangka_Waktu, Buffer_Stock, Jenis_PKP'
      'FROM TRIAL.dbo.Supplier'
      'WHERE Kode_Sup = :NEW_Kode_Sup')
    ModifySQL.Strings = (
      'UPDATE TRIAL.dbo.Supplier'
      
        'SET Kode_Sup = :NEW_Kode_Sup, Nama_Sup = :NEW_Nama_Sup, Alamat =' +
        ' :NEW_Alamat, '
      
        '  Telp = :NEW_Telp, Fax = :NEW_Fax, Kota = :NEW_Kota, Contact_Pe' +
        'rson = :NEW_Contact_Person, '
      
        '  Jangka_Waktu = :NEW_Jangka_Waktu, Buffer_Stock = :NEW_Buffer_S' +
        'tock, '
      '  Keterangan = :NEW_Keterangan, No_Rekening = :NEW_No_Rekening, '
      '  Nama_Bank = :NEW_Nama_Bank, Jenis_PKP = :NEW_Jenis_PKP, '
      
        '  NPWP = :NEW_NPWP, NPPKP = :NEW_NPPKP, Nama_Perusahaan = :NEW_N' +
        'ama_Perusahaan, '
      '  Alamat_Perusahaan = :NEW_Alamat_Perusahaan'
      'WHERE Kode_Sup = :OLD_Kode_Sup;'
      'SELECT Jangka_Waktu, Buffer_Stock, Jenis_PKP'
      'FROM TRIAL.dbo.Supplier'
      'WHERE Kode_Sup = :NEW_Kode_Sup')
    DeleteSQL.Strings = (
      'DELETE FROM TRIAL.dbo.Supplier'
      'WHERE Kode_Sup = :OLD_Kode_Sup')
    FetchRowSQL.Strings = (
      
        'SELECT Kode_Sup, Nama_Sup, Alamat, Telp, Fax, Kota, Contact_Pers' +
        'on, '
      
        '  Jangka_Waktu, Buffer_Stock, Keterangan, No_Rekening, Nama_Bank' +
        ', '
      '  Jenis_PKP, NPWP, NPPKP, Nama_Perusahaan, Alamat_Perusahaan'
      'FROM TRIAL.dbo.Supplier'
      'WHERE Kode_Sup = :Kode_Sup')
    Left = 656
    Top = 200
  end
